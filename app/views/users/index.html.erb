<h2>List Users</h2>

<table>
  <tr><th>Username<th>Person<th>Admin?
  </tr>

<% for user in @users %>
  <tr>
  	<td><%= link_to user.username, user %></td>
		<td><%= user.person.full_name%></td>
		<td><%= user.administrator %></td>
  </tr>
<% end %>
</table>

<p class="navigation">
<%= link_to 'New user', new_user_path %>
</p>
