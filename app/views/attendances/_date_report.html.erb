<p><%= select(:service, :id, @services.collect { |s| [s.service.strftime("%a, %b %d, %Y @%r"),
	s.id] }, { :include_blank => true }) %></p>

<%= observe_field :service_id, :url => { :action => :change_date_report },
  :with => "'&network_id=' + $('network_id').value + '&service_id=' + $('service_id').value" %>
